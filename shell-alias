alias c='clear'
alias e='exit'
alias g='grep'
alias gi='grep -i '
alias gr='grep -r '
alias h='history'
alias l='ls --color'
alias la='ls -A --color'
alias ll='ls -l --color'
alias p='clear; ls -l --color'
alias rm='rm -i '

alias gbr='git branch'
alias gco='git-co.sh'
alias gg='gitk &'
alias ggall='gitk --all &'
alias ggh='gitk . &'
alias ggui='git gui &'
alias gst='git status'
